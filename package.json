{
  "name": "trace.moe.ts",
  "version": "1.3.9",
  "description": "An API wrapper for https://trace.moe with typings",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "author": "Jan Leigh <janleigh@pm.me>",
  "license": "GPL-3.0",
  "homepage": "https://github.com/TheRealKizu/trace.moe.ts#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheRealKizu/trace.moe.ts.git"
  },
  "bugs": {
    "url": "https://github.com/TheRealKizu/trace.moe.ts/issues"
  },
  "scripts": {
    "build": "yarn build:clean && yarn format:check && yarn tsc",
    "build:clean": "yarn rimraf ./dist/",
    "format": "prettier --write lib",
    "format:check": "prettier --check lib",
    "lint": "eslint . --ignore-path .gitignore --ext .ts",
    "lint:fix": "eslint . --ignore-path .gitignore --ext .ts --fix",
    "prepublishOnly": "yarn lint && yarn build && yarn test",
    "test": "mocha --require ts-node/register tests/*.spec.ts --exit"
  },
  "keywords": [
    "trace-moe",
    "anime",
    "api-wrapper",
    "typescript",
    "nodejs"
  ],
  "dependencies": {
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.7.5",
    "@types/node-fetch": "^2.5.12",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "chai": "^4.3.4",
    "eslint": "^8.1.0",
    "mocha": "^9.1.3",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  }
}
